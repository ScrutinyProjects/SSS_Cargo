@{
    ViewBag.Title = "SSS Cargo - Booking Reports";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Assets/css/jquery-ui.min.css" rel="stylesheet" />
<div class="breadcrumbs" id="breadcrumbs">
    <ul class="breadcrumb">
        <li>
            Home
        </li>
        <li class="active" style="font-size:14px; font-weight:600">Booking Report</li>
    </ul>
</div>
<div class="page-content">
    <input type="hidden" id="hiddenExpenseId" />
    <input type="hidden" id="hiddenloginid" value="@ViewBag.LoginId" />
    <input type="hidden" id="hiddencounterid" value="@ViewBag.CounterId" />
    <div class="row">
        <div class="col-xs-12">
            <!-- PAGE CONTENT BEGINS -->
            <div class="alert alert-warning" style="display: none;" id="divWarningAlert">
                <strong>Warning!</strong>
                <span id="spanWarningAlert"></span>
            </div>
            <div class="alert alert-danger" style="display: none;" id="divErrorAlert">
                <strong>Error!</strong>
                <span id="spanErrorAlert"></span>
            </div>
            <div class="alert alert-success" style="display: none;" id="divSuccessAlert">
                <strong>Success!</strong>
                <span id="spanSuccessAlert"></span>
            </div>

            <div class="panel">

                <div class="panel-body">
                    <div class="form-group">

                        <div class="col-sm-3">
                            <label class="control-label" style="text-align: left;" for="txtFromDate">From Date: <span class="red">*</span> </label>
                            <input type="text" id="txtFromDate" maxlength="10" readonly placeholder="From Date" class="form-control numeric" />
                            <span class="red" id="spnFromDate"></span>
                        </div>

                        <div class="col-sm-3">
                            <label class="control-label" style="text-align: left;" for="txtToDate">To Date: <span class="red">*</span> </label>
                            <input type="text" id="txtToDate" maxlength="10" readonly placeholder="To Date" class="form-control numeric" />
                            <span class="red" id="spnToDate"></span>
                        </div>
                        <div class="col-sm-3">
                            <label class="control-label" style="text-align: left;" for="ddlLocation">Location <span class="red">*</span> </label>
                            <select class="form-control" id="ddlLocation">
                                <option value="-1">Select Location</option>
                            </select>
                            <span class="red" id="spnLocation"></span>
                        </div>
                        <div class="col-sm-3">
                            <label class="control-label" style="text-align: left;" for="ddlGCType">GC Type <span class="red">*</span> </label>
                            <select class="form-control" id="ddlGCType">
                                <option value="-1">Select GC Type</option>
                            </select>
                            <span class="red" id="spnGCtype"></span>
                        </div>
                        <div class="col-sm-3">
                            <label class="control-label" style="text-align: left;" for="ddlBookingStatus">Booking Status <span class="red">*</span> </label>
                            <select class="form-control" id="ddlBookingStatus">
                                <option value="-1">Select Booking Status</option>
                            </select>
                            <span class="red" id="spnBookingStatus"></span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel">
                <div class="panel-body">
                    <div class="clearfix form-actions form-group" style="background-color: #fff;">
                        <div style="width: 95%;">
                            <button class="btn btn-sm btn-info pull-right" id="btnSearch" type="button" style="margin: 5px;" onclick="return SaveExpense()">
                                <i class="ace-icon fa fa-check bigger-110"></i>
                                Search
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <span id="lblGridMessage" style="display:none;">No Records Found</span>
    <div id="divExpenses">
        <div id="gridExpenses">
        </div>
    </div>
</div>
    <script src="~/Assets/js/jquery.2.1.1.min.js"></script>
    <script src="~/Assets/js/jquery-ui.min.js"></script>
    <script type="text/javascript">window.jQuery || document.write("<script src='~/Assets/js/jquery.min.js'>" + "<" + "/script>");</script>
    <script type="text/javascript">if ('ontouchstart' in document.documentElement) document.write("<script src='~/Assets/js/jquery.mobile.custom.min.js'>" + "<" + "/script>");</script>
    <script src="~/Assets/js/bootstrap.min.js"></script>
    <link href="~/Content/kendo/2014.1.318/kendo.default.min.css" rel="stylesheet" />
    <link href="~/Content/kendo/2014.1.318/kendo.common.min.css" rel="stylesheet" />
    @*<script src="~/scripts/kendo/2014.1.318/jquery.min.js"></script>*@
    <script src="~/scripts/kendo/2014.1.318/kendo.web.min.js"></script>
    <script src="~/scripts/common.js"></script>
    <script src="~/scripts/bookingreports.js"></script>

    <script type="text/javascript">
        jQuery(function ($) {
            getBookingReportsData();
        });

    </script>

