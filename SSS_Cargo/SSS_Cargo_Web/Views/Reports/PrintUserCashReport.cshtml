
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PrintUserCashReport</title>
</head>
<body>
    <div> 
        <input type="hidden" id="hiddenloginid" name="hiddenloginid" value="@ViewBag.LoginId" />
        <input type="hidden" id="hiddencounterid" name="hiddencounterid" value="@ViewBag.CounterId" />
        <input type="hidden" id="hiddentransactiondate" value="@ViewBag.TransactionDate" />
        <input type="hidden" id="hiddenlocationid" value="@ViewBag.LocationId" />
        <input type="hidden" id="hiddenuserid" value="@ViewBag.UserId" />

        <div class="form-group">
            <div class="col-sm-12" id="divUserCashReport" margin:10px;">
                <label class="control-label" style="text-align: left;">User cash Report </label>
                <label class="control-label" id="lblUserName" style="text-align: left;"> </label>,
                Date of transaction is 
                <label id="lblTransactionDate"></label>
                location:
                <label id="lblLocation"></label>
                Print Date: @DateTime.Now
            </div>
            <div class="col-xs-12" style="overflow-x:auto;">
                <table id="dynamic-table" class="table table-striped table-bordered table-hover" style="margin-bottom: 0;">
                    <thead>
                        <tr>
                            <th style="width: 5%;">S.No.</th>
                            <th style="width: 15%;">GC No.</th>
                            <th style="width: 15%;">GC Type</th>
                            <th style="width: 15%;">Articles</th>
                            <th style="width: 15%;">Transaction Date</th>
                            <th style="width: 15%;">Transaction</th>
                            <th style="width: 15%;">From/To</th>
                            <th style="width: 15%;">Bill Amount</th>
                            <th style="width: 15%;">Driver Charges</th>
                            <th style="width: 15%;">Hamali Charges</th>
                            <th style="width: 15%;">Pass Charges</th>
                            <th style="width: 15%;">SUR Charges</th>
                            <th style="width: 15%;">LR Charges</th>
                            <th style="width: 15%;">Collection Charges</th>
                            <th style="width: 15%;">Booking Charges</th>
                            <th style="width: 15%;">Pickup Charges</th>
                            <th style="width: 15%;">Delivery Charges</th>
                            <th style="width: 15%;">Demo Charges</th>
                            <th style="width: 15%;">Total Amount</th>
                            <th style="width: 15%;">Remarks</th>
                        </tr>
                    </thead>
                    <tbody id="tbodyusercashrecords">
                        <tr>
                            <td colspan="20" style="text-align:center">No User Cash records found</td>
                        </tr>
                    </tbody>
                </table>


            </div>
            <div class="col-xs-12" style="overflow-x:auto;">

                <br><br />
                <h3>Summary:</h3>
                <table id="dynamic-table" class="table table-striped table-bordered table-hover" style="margin-bottom: 0;">
                    <thead>
                        <tr>
                            <th style="width: 5%;">S.No.</th>
                            <th style="width: 15%;">Transaction</th>
                            <th style="width: 15%;">LRS</th>
                            <th style="width: 15%;">Articles</th>
                            <th style="width: 15%;">Bill Amount</th>
                        </tr>
                    </thead>
                    <tbody id="tbodyusercashsummary">
                        <tr>
                            <td colspan="5" style="text-align:center">No User Cash summary found</td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div>
                Cash to be handover : <span id="spnTotalCashHandover"></span>
                Remarks: 
                Cash Paid: <span id="spnTotalCashPaid"></span>
                Print Date & Time: @DateTime.Now
                Operator Signature & Name:
                Incharge Signature & Name:
            </div>
        </div>
    </div>
    <script src="~/Assets/js/jquery.2.1.1.min.js"></script>
    <script src="~/Assets/js/jquery-ui.min.js"></script>
    <script type="text/javascript">window.jQuery || document.write("<script src='~/Assets/js/jquery.min.js'>" + "<" + "/script>");</script>
    <script type="text/javascript">if ('ontouchstart' in document.documentElement) document.write("<script src='~/Assets/js/jquery.mobile.custom.min.js'>" + "<" + "/script>");</script>
    <script src="~/Assets/js/bootstrap.min.js"></script>
    <script src="~/scripts/common.js"></script>
    <script src="~/scripts/usercashreport.js"></script>
    <script type="text/javascript">
    jQuery(function ($) {
        PrintUserCashReport();
    });

    </script>
</body>
</html>
