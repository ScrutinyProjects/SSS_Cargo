@{
    ViewBag.Title = "SSS Cargo - Booking";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/Assets/css/jquery-ui.min.css" rel="stylesheet" />

<div class="page-content">
    <div class="row">
        <div class="col-xs-12">
            <!-- PAGE CONTENT BEGINS -->
           
                <div class="panel">
                    <div class="panel-body" style="padding: 30px 10px;">
                        <div class="form-group">
                            <label class="col-sm-12 control-label" style="text-align: left;font-size: 15px;">Thank you, Consignee booked successfully and Consignee No. is : <span style="text-align: center;font-weight:600" id="spanbookingnumber">@ViewBag.BookingSerialNumber  </span> </label>
                        </div>
                        <div class="form-group" style="padding: 30px 10px;">
                            <button class="btn" onclick="return newbooking()">Close & Add new Booking</button>
                            <button class="btn btn-danger" onclick="return printbookingconfirmation()">Print</button>
                        </div>
                    </div>
                </div>
           
        </div>
    </div>
</div>
<script src="~/Assets/js/jquery.2.1.1.min.js"></script>
<script src="~/Assets/js/jquery-ui.min.js"></script>
<script type="text/javascript">
    window.jQuery || document.write("<script src='~/Assets/js/jquery.min.js'>" + "<" + "/script>");
</script>
<script type="text/javascript">
    if ('ontouchstart' in document.documentElement) document.write("<script src='~/Assets/js/jquery.mobile.custom.min.js'>" + "<" + "/script>");
</script>
<script src="~/Assets/js/bootstrap.min.js"></script>
<script type="text/javascript">
    function printbookingconfirmation() {

    }
    function newbooking() {
        window.location = "/cargo/booking";
    }
</script>
