@{
    ViewBag.Title = "SSS Cargo - Booking";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/Assets/css/jquery-ui.min.css" rel="stylesheet" />

<div class="breadcrumbs" id="breadcrumbs">
    <ul class="breadcrumb">
        <li>
            Home
        </li>
        <li class="active" style="font-size:14px; font-weight:600">
            Booking
        </li>
    </ul>
    <div class="form-group" style="float:right;width: 25%;
    padding: 3px 10px;">
        <div class="col-sm-12">
            <select class="form-control" id="selectgctype" onchange="showmeasurement(this)">
                <option value="0">Select GC Type</option>
            </select>
            <span class="red" id="spangctypehelper"></span>
        </div>
    </div>
</div>
<div class="page-content">
    <input type="hidden" id="hiddenbookid" value="0" />
    <input type="hidden" id="hiddencounterid" value="@ViewBag.CounterId" />
    <input type="hidden" id="hiddenloginid" value="@ViewBag.LoginId" />
    <input type="hidden" id="hiddenmobilenumber" value="@ViewBag.ContactNumber" />
    <div class="row">
        <div class="col-xs-12">
            <!-- PAGE CONTENT BEGINS -->
            <div class="alert alert-warning" style="display: none;" id="divWarningAlert">
                <strong>Warning!</strong>
                <span id="spanWarningAlert"></span>
            </div>
            <div class="alert alert-danger" style="display: none;" id="divErrorAlert">
                <strong>Error!</strong>
                <span id="spanErrorAlert"></span>
            </div>
            <div class="alert alert-success" style="display: none;" id="divSuccessAlert">
                <strong>Success!</strong>
                <span id="spanSuccessAlert"></span>
            </div>

            <div class="panel">
                <div class="panel-body">
                    <div class="form-group">
                        <div class="col-sm-3">
                            <label class="control-label" style="text-align: left;" for="textsendercounter">Sending From <span class="red">*</span> </label>
                            <input type="text" id="textsendercounter" maxlength="50" value="@ViewBag.CounterName" placeholder="From" class="form-control" disabled />
                            <span class="red" id="spansendercounter"></span>
                        </div>
                        <div class="col-sm-3">
                            <label class="control-label" style="text-align: left;" for="textsendermobile">Sender Mobile Number <span class="red">*</span> </label>
                            <input type="text" id="textsendermobile" maxlength="10" value="" placeholder="Sender Mobile Number" class="form-control mobile-number" />
                            <span class="red" id="spansendermobile"></span>
                        </div>
                        <div class="col-sm-3">
                            <label class="control-label" style="text-align: left;" for="textsendername">Sender Name <span class="red">*</span> </label>
                            <input type="text" id="textsendername" maxlength="50" value="" placeholder="Sender Name" class="form-control" />
                            <span class="red" id="spansendername"></span>
                        </div>
                        <div class="col-sm-3">
                            <label class="control-label" style="text-align: left;" for="textsenderaddress">Sender Address <span class="red">*</span> </label>
                            <input type="text" id="textsenderaddress" maxlength="100" value="" placeholder="Sender Address" class="form-control" />
                            <span class="red" id="spansenderaddress"></span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel">
                <div class="panel-body">
                    <div class="form-group">
                        <div class="col-sm-3">
                            <label class="control-label" style="text-align: left;" for="textreceivercounter">Receive To <span class="red">*</span> </label>
                            <input type="text" id="textreceivercounter" maxlength="50" value="" placeholder="To" class="form-control" onblur="getroutedetails()" />
                            <span class="red" id="spanreceivercounter"></span>
                        </div>
                        <div class="col-sm-3">
                            <label class="control-label" style="text-align: left;" for="textreceivermobile">Receiver Mobile Number <span class="red">*</span> </label>
                            <input type="text" id="textreceivermobile" maxlength="10" value="" placeholder="Mobile Number" class="form-control mobile-number" />
                            <span class="red" id="spanreceivermobile"></span>
                        </div>
                        <div class="col-sm-3">
                            <label class="control-label" style="text-align: left;" for="textreceivername">Receiver Name <span class="red">*</span> </label>
                            <input type="text" id="textreceivername" maxlength="50" value="" placeholder="Name" class="form-control" />
                            <span class="red" id="spanreceivername"></span>
                        </div>
                        <div class="col-sm-3">
                            <label class="control-label" style="text-align: left;" for="textreceiveraddress">Receiver Address <span class="red">*</span> </label>
                            <input type="text" id="textreceiveraddress" maxlength="100" value="" placeholder="Address" class="form-control" />
                            <span class="red" id="spanreceiveraddress"></span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel">
                <div class="panel-body">
                    <div class="form-group">
                        <div class="col-sm-2">
                            <label class="control-label" style="text-align: left;" for="textactualweight">Actual Weight In <label class="labelactualweightin">Kgs</label> <span class="red">*</span> </label>
                            <input type="text" id="textactualweight" maxlength="4" value="" placeholder="Actual Weight" class="form-control numeric" onblur="updatetotalweight()" />
                            <span class="red" id="spanactualweight"></span>
                        </div>
                        <div class="col-sm-2">
                            <label class="control-label" style="text-align: left;" for="textpieces">Pieces <span class="red">*</span> </label>
                            <input type="text" id="textpieces" maxlength="5" value="" placeholder="Pieces" class="form-control numeric" onblur="updatetotalweight()" />
                            <span class="red" id="spanpieces"></span>
                        </div>
                        <div class="col-sm-3">
                            <label class="control-label" style="text-align: left;" for="selectpiecestype">Pieces Type <span class="red">*</span> </label>
                            <select class="form-control" id="selectpiecestype">
                                <option value="0">Select Pieces Type</option>
                            </select>
                            <span class="red" id="spanpiecestype"></span>
                        </div>
                        <div class="col-sm-2">
                            <label class="control-label" style="text-align: left;" for="textactualweight">Actual Weight In <label class="labelactualweightin">Kgs</label><span class="red">*</span> </label>
                            <input type="text" id="textdisplayactualweight" maxlength="4" value="" placeholder="Actual Weight" class="form-control numeric" disabled />
                            <span class="red" id="spanactualweight"></span>
                        </div>
                        <div class="col-sm-2">
                            <label class="control-label" style="text-align: left;" for="textcalculatedweight">Calculated Weight In <label class="labelactualweightin">Kgs</label><span class="red">*</span> </label>
                            <input type="text" id="textcalculatedweight" maxlength="6" value="" placeholder="Calculated Weight " class="form-control numeric" disabled />
                            <span class="red" id="spancalculatedweight"></span>
                        </div>
                        <div style="float:left">
                            <button class="btn btn-sm btn-success pull-right" type="button" style="margin: 25px 0px 4px 8px;" onclick="return addparcelitems()">
                                <i class="ace-icon fa fa-plus bigger-110"></i>
                                Add
                            </button>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-xs-12">
                            <table id="dynamic-table" class="table table-striped table-bordered table-hover" style="margin-bottom: 0;">
                                <thead>
                                    <tr>
                                        <th style="width: 5%;">S.No.</th>
                                        <th style="width: 15%;">Actual Weight</th>
                                        <th style="width: 15%;">Pieces</th>
                                        <th style="width: 15%;">Pieces Type</th>
                                        <th style="width: 15%;">Total Weight</th>
                                        <th style="width: 10%;">Actions</th>
                                    </tr>
                                </thead>
                                <tbody id="tbodyparcelitems">
                                    <tr>
                                        <td colspan="6" style="text-align:center">No parcel items added</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel">
                <div class="panel-body">
                    <div class="form-group">
                        <div class="col-sm-3">
                            <label class="control-label" style="text-align: left;" for="textpoint1">Transhipment Point 1 </label>
                            <input type="text" id="textpoint1" maxlength="50" value="" placeholder="Transhipment Point 1" class="form-control" onblur="getroutedetails()" />
